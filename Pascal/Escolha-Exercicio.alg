algoritmo "Escolha"


var
voto: literal

inicio

Escreval("Digite seu voto")
leia(voto)

Escolha voto
caso "20"
     Escreval("Você votou em Marcelo Bernart")
caso "30"
     Escreval("Você votou em Pernalonga")
caso "40"
     Escreval("Você votou no Patolino")
outrocaso
         Escreval("Voto Nulo")
fimescolha
fimalgoritmo
